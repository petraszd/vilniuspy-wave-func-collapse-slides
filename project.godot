config_version=4

_global_script_classes=[ {
"base": "Control",
"class": "WFCCollapser",
"language": "GDScript",
"path": "res://Scripts/WFCCollapser.gd"
}, {
"base": "Node",
"class": "WFCImageData",
"language": "GDScript",
"path": "res://Scripts/WFCImageData.gd"
}, {
"base": "Node2D",
"class": "WFCTile",
"language": "GDScript",
"path": "res://Scripts/WFCTile.gd"
} ]
_global_script_class_icons={
"WFCCollapser": "",
"WFCImageData": "",
"WFCTile": ""
}

[application]

config/name="Wave Function Collapse Demo"
run/main_scene="res://Scenes/Main.tscn"
config/icon="res://icon.png"

[autoload]

WFC="*res://Scripts/WFC.gd"

[display]

window/size/width=2048
window/size/height=1200
window/dpi/allow_hidpi=true
window/stretch/mode="2d"
window/stretch/aspect="expand"

[gui]

theme/use_hidpi=true

[physics]

common/enable_pause_aware_picking=true

[rendering]

2d/options/use_software_skinning=false
environment/default_environment="res://default_env.tres"
